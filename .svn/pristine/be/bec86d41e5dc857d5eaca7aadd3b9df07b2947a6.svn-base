body {background-color:#ffffff; font-family: Arial,Helvetica,Verdana; font-size: 12px; color: #000; min-width: 650px;}

input,select,button {font-size:12px;color:#333;}

#container input[type="checkbox"] {margin: 0 3px 0 0;padding: 0;vertical-align: middle;}
#container input:not([type]), #container input[type="color"], #container input[type="email"], #container input[type="number"], 
#container input[type="password"], #container input[type="tel"], #container input[type="url"], #container input[type="text"]  
{outline: none; border: 1px solid #999; font-family: Arial,Helvetica,Verdana;font-size: 100%; padding: 2px 0; margin: 0; width: 100%;}

p {padding:0; margin:12px 0;}
div {display: block;}
.logo
{
font-size: 28px;
color: #7a7573;
font-weight: bold;
padding: 0px;
margin: 0px;
display: inline-block;
}
h1,h2,h3,h4,h5
{
display: block;
color: #7a7573;
font-weight: normal;
padding: 0px;
margin: 0px;
}
h1 a,h2 a,h3 a,h4 a,h5 a, .logo a
{
    text-decoration: none;
    color: #7a7573;
    font-weight: normal;
    padding: 0px;
    margin: 0px;
}

h1 { font-size: 2.1em; }
h3 { font-size: 1.5em; }


#form_wrapper{background-color:#fff;margin:0px;padding:0px;}
/*Main layout*/
#container
{
    min-width: 998px;
    padding: 14px 18px 18px;
    margin: 0 auto;
}
#rightCol {
float: right;
margin: 0 0 22px 20px;
width: 270px;
}
#leftCol {
margin-right: 320px;
}
/**/
.status {display: block; float: right;}
/*Forms */
.formList 
{
    color: #666;
    padding: 30px 50px 30px 50px;
    background-color: #f0ebea;    
}
.box
{
    border-radius: 4px;
    box-shadow: 0 0 0 4px #ddd;
    padding: 5px 10px;
}
.box h3 {text-align: center;margin-top:7px;}
input.field{ width:190px !important; }
.alert { color: Red; }
.form_head {margin-top: 10px !important;vertical-align: middle !important; margin-bottom: 15px !important; }
.form_row {vertical-align: middle !important; margin-bottom: 15px !important; }
/*end Forms*/

/*Login form*/
.go_right {float:right;}
.go_left {float:left;}
p.labeled_field {vertical-align: middle; line-height: 20px;}
a.recover_pwd {margin-left: 55px;}
p.rememberme {text-align: right;}
a.registration {text-align: center; padding: 15px 0 15px 0; font-size: 1.5em; border-top: 1px solid #ddd; width: 100%; display: block;}
/*end Login form*/

/*Centered form*/
.centered_form_head {margin-top: 10px !important;text-align: center; vertical-align: middle !important; margin-bottom: 15px !important; }
.centered_form_row {text-align: center; vertical-align: middle !important; margin-bottom: 15px !important; }
.col1 {display: inline-block; width: 160px; padding: 0 0 0 0 !important; vertical-align: middle !important; text-align: right;}
.col2 {display: inline-block; width: 320px; margin: 0 0 0 15px !important; vertical-align: middle !important; text-align: left;}
.col2l {display: inline-block; width: 460px; margin: 0 0 0 15px !important; vertical-align: middle !important; text-align: left;}
.col3 {display: inline-block; text-align: left; width: 130px; margin: 0 0 0 5px !important;  }
/*end Centered form */




